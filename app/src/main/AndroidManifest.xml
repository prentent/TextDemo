<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.textdemo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".reciver.MyReciver">
            <intent-filter>
                <action android:name="android.intent.action.CONFIGURATION_CHANGED" />
            </intent-filter>
        </receiver>

        <activity android:name=".json_xml.JXActivity" />
        <activity android:name=".animation.dhActivity" />
        <activity android:name=".caream.CaremActivity" />

        <provider
            android:name=".contprovider.MyContProvider"
            android:authorities="lh_ce"
            android:exported="true" />

        <activity android:name=".connection.AidlActivity" />
        <activity android:name=".connection.ConActivity" />

        <service android:name=".service.LocalService" />

        <activity android:name=".ouerView.MyViewActivity" />
        <activity android:name=".connection.MessengerActivity" />
        <activity android:name=".udp.UdpActivity"></activity>
    </application>

</manifest>